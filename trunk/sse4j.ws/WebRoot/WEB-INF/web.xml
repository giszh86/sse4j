<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<listener>
		<listener-class>com.sun.xml.ws.transport.http.servlet.WSServletContextListener</listener-class>
	</listener>
	<servlet>
		<description>JAX-WS endpoint - RoutingService</description>
		<display-name>RoutingService</display-name>
		<servlet-name>RoutingService</servlet-name>
		<servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>HotTile</servlet-name>
		<servlet-class>org.sse.http.HotTile</servlet-class>
	</servlet>
	<servlet>
		<description>TCrawl</description>
		<display-name>TCrawl</display-name>
		<servlet-name>TCrawling</servlet-name>
		<servlet-class>org.sse.http.TCrawling</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>RoutingService</servlet-name>
		<url-pattern>/MatchingPort</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RoutingService</servlet-name>
		<url-pattern>/LocatingPort</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RoutingService</servlet-name>
		<url-pattern>/SearchingPort</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RoutingService</servlet-name>
		<url-pattern>/RoutingPort</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HotTile</servlet-name>
		<url-pattern>/servlet/HotTile</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TCrawling</servlet-name>
		<url-pattern>/servlet/TCrawling</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>sse4j.html</welcome-file>
	</welcome-file-list>
</web-app>
